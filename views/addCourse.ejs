<%- include('header') %>
<form method="post" action="/addCourse/<%= vu_id %>/<%= tip_ust %>">
    
   
    <div class="form-group">
        <label for="courseCode">Course Code</label>
        <input type="text" class="form-control" id="courseCode" name="courseCode">
    </div>
    <div class="form-group">
        <label for="coursVersion">Course Version</label>
        <input type="text" class="form-control" id="coursVersion" name="coursVersion">
    </div>
    <div class="form-group">
        <label for="coursName">Course Name</label>
        <input type="text" class="form-control" id="coursName" name="coursName">
    </div>
    
    <button type="submit" class="btn btn-primary">Submit</button>
</form>

<%- include('footer') %>